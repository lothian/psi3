
.PHONY:	default all install depend clean targetclean

default: all

all:

install_inc:
depend:
clean:
targetclean:

top_srcdir = @top_srcdir@
prefix = @prefix@
srcdir = @srcdir@
datadir = @datadir@
rubydatadir = $(datadir)/ruby
datarootdir = @datarootdir@
VPATH = @srcdir@
top_objdir = ../..
MKDIRS = @MKDIRS@
INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@

install: ccenergy.rb ccsort.rb cctriples.rb cints.rb clean.rb input.rb optking.rb pes.rb psi3.rb scf.rb symbols.rb transqt.rb chkpt.rb
	$(MKDIRS) $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/chkpt.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/ccenergy.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/ccsort.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/cctriples.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/cints.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/clean.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/input.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/optking.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/pes.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/psi3.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/scf.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/symbols.rb $(rubydatadir)
	$(INSTALL_DATA) $(srcdir)/transqt.rb $(rubydatadir)

install_man: 

$(top_srcdir)/configure: $(top_srcdir)/configure.ac $(top_srcdir)/aclocal.m4
	cd $(top_srcdir) && autoconf

$(top_objdir)/config.status: $(top_srcdir)/configure
	cd $(top_objdir) && ./config.status --recheck

Makefile: $(srcdir)/Makefile.in $(top_objdir)/config.status
	cd $(top_objdir) && CONFIG_FILES=lib/Makefile ./config.status
