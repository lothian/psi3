srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LDLIBS += $(LAPACK) $(BLAS)

PSILIBS = -lPSI_dpd -lPSI_qt -lPSI_chkpt -lPSI_iwl -lPSI_psio -lPSI_ciomr -lPSI_ipv1

CSRC = \
cceom.c       diag.c                   diagSS.c       cache.c         hbar_extra.c \
check_sum.c   form_diagonal.c          norm.c         get_moinfo.c    rzero.c \
dgeev_eom.c   get_eom_params.c         schmidt_add.c  c_clean.c       cache.c \
c_clean.c     precondition.c           hbar_norms.c   get_params.c     \
sigmaSS.c     sigmaSD.c    FSD.c       WmnieSD.c    WamefSD.c  \
sigmaDS.c     WmaijDS.c    WabejDS.c   WnmjeDS.c    WbmfeDS.c  \
sigmaDD.c     FDD.c        WmnijDD.c   WabefDD.c    WmbejDD.c  WmnefDD.c \
local.c restart.c sort_C.c amp_write.c read_guess.c local_guess.c  sort_amps.c  \
write_Rs.c

BINOBJ = $(CSRC:%.c=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

