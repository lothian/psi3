srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LDLIBS += $(BLAS)

PSILIBS = libPSI_qt.a libPSI_chkpt.a libPSI_iwl.a -lPSI_psio libPSI_ciomr.a libPSI_ipv1.a

CSRC = 

CXXSRC = clag.cc lagcalc.cc ci_energy.cc

BINOBJ = $(CSRC:%.c=%.o) $(CXXSRC:%.cc=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: clag.1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1
