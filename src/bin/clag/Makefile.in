srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LD = $(CXX)
LDLIBS += $(BLAS)

PSILIBS = libPSI_qt.a libPSI_chkpt.a libPSI_iwl.a -lPSI_psio libPSI_ciomr.a libPSI_ipv1.a

CSRC = clag.c lagcalc.c ci_energy.c \
       onel_to_cas.c twoel_to_cas.c onepdm_to_cas.c twopdm_to_cas.c \
       lag_to_cas.c

CXXSRC = 

BINOBJ = $(CSRC:%.c=%.o) $(CXXSRC:%.cc=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: clag.1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1
