srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LD = $(CXX)
LDLIBS += $(LAPACK) $(BLAS)

PSILIBS = -lPSI_file30 -lPSI_chkpt -lPSI_qt -lPSI_ciomr -lPSI_psio -lPSI_ipv1

CXXSRC = dboc.cc geom.cc print.cc overlap.cc mo_overlap.cc linalg.cc rhf.cc \
	rci.cc
#	rohf.cc uhf.cc rccsd.cc rci.cc

BINOBJ = $(CXXSRC:%.cc=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

