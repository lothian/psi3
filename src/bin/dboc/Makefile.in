srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LD = $(CXX)
LDLIBS += $(LAPACK) $(BLAS)

PSILIBS = -lPSI_basis -lPSI_qt -lPSI_chkpt -lPSI_psio -lPSI_ciomr -lPSI_ipv1

CXXSRC = dboc.cc geom.cc print.cc moinfo.cc overlap.cc mo_overlap.cc linalg.cc\
	rhf.cc rohf.cc uhf.cc rci.cc roci.cc
#	rccsd.cc

BINOBJ = $(CXXSRC:%.cc=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

