srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LD = $(F77)
LDLIBS += $(BLAS)

PSILIBS = libqt.a libfile30.a libiwl.a -lpsio libciomr.a libipv1.a

CSRC = get_mo_info.c ints.c setup_io.c params.c read_dens.c \
read_lag.c ref_orbs.c f_act.c gradient.c hessian.c step.c \
thetas.c cleanup.c diis.c 

CXXSRC = detcas.cc indpairs.cc

BINOBJ = $(CSRC:%.c=%.o) $(CXXSRC:%.cc=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1
