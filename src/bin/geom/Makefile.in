srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSILIBS = libPSI_ciomr.a libPSI_ipv1.a

CSRC = geom.c read11.c read_geom.c gprgid.c

BINOBJ = $(CSRC:%.c=%.o)

