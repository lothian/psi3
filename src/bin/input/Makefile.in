srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LD = $(CC)

PSILIBS = -lqt -lciomr -lipv1 -lmalloc

CINC += -DSHARE=\"$(prefix)/share/pbasis.dat\"

CSRC = input.c parsing.c read_cart.c read_zmat.c reorient.c \
find_symmetry.c count_uniques.c functions.c sym_operations.c atom_num.c \
init_elem_names.c read_basis.c build_transmat.c build_cart2pureang.c \
build_so_classes.c build_usotao.c write_to_file30.c

BINOBJ = $(CSRC:%.c=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

