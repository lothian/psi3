srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars
                                                                                
LDLIBS += $(LAPACK) $(BLAS)

PSILIBS = -lPSI_dpd -lPSI_qt -lPSI_chkpt -lPSI_iwl -lPSI_psio -lPSI_ciomr -lPSI_ipv1

CSRC = cache.c priority.c get_moinfo.c get_params.c amps.c \
energy.c opdm.c dipole.c lag.c mohess.c Z.c W.c sort_opdm.c \
sort_W.c twopdm.c check_energy.c sort_twopdm.c mp2.c

BINOBJ = $(CSRC:%.c=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: mp2.1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

