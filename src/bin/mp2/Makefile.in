srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars
                                                                                
LDLIBS += $(LAPACK) $(BLAS)

PSILIBS = -lPSI_dpd -lPSI_qt -lPSI_chkpt -lPSI_iwl -lPSI_psio -lPSI_ciomr -lPSI_ipv1

CSRC = cache.c priority.c get_moinfo.c get_params.c energy.c opdm.c mp2.c

BINOBJ = $(CSRC:%.c=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

