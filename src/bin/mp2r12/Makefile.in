srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LDLIBS += $(BLAS)

PSILIBS = -lPSI_qt -lPSI_file30 -lPSI_chkpt -lPSI_iwl -lPSI_psio -lPSI_ciomr -lPSI_ipv1

CSRC = gprgid.c mp2r12.c mp2r12_energy.c

BINOBJ = $(CSRC:%.c=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: mp2r12.1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

