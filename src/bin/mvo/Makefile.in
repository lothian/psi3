srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LD = $(CXX)

PSILIBS = libPSI_qt.a libPSI_chkpt.a libPSI_iwl.a libPSI_psio.a libPSI_ciomr.a libPSI_ipv1.a

CSRC = mvo.c gprgid.c transform.c get_mp2nos.c

CXXSRC = 

BINOBJ = $(CSRC:%.c=%.o) $(CXXSRC:%.cc=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
