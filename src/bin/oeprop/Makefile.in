srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

#Need do remove -DAIX because I have a variable named AIX
CDEF_NOAIX = $(CDEF:-DAIX=)
CFLAGS = $(CDBG) $(COPT) $(CDEF_NOAIX) $(COTH) $(CINC)

LD = $(CC)

PSILIBS = -lPSI_file30 -lPSI_qt -lPSI_ciomr -lPSI_psio -lPSI_ipv1

CSRC = main.c oeprop.c parsing.c grid_unitvec.c read_density.c \
compute_density.c read_basset.c recursion.c populate.c mprefxyz.c \
initialize.c read_zvec.c overlap.c get_nmo.c grid.c grid_oeprop.c \
grid_dens_2d.c grid_dens_3d.c grid_mo.c print.c print_grid.c \
connectivity.c

BINOBJ = $(CSRC:%.c=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

