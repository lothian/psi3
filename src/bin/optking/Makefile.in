srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LD = $(CXX)

PSILIBS = -lPSI_chkpt -lPSI_psio -lPSI_ciomr -lPSI_ipv1

CSRC = gprgid.c char_table.c

CXXSRC = opt.cc compute_B.cc compute_G.cc compute_H.cc compute_q.cc \
         new_geom.cc empirical_H.cc salc.cc internals.cc cartesians.cc \
         delocalize.cc irrep.cc \

BINOBJ = $(CSRC:%.c=%.o) $(CXXSRC:%.cc=%.o)
ALLOC = 

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: $(TARGET).1
	$(MKDIRS) $(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(mandir)/man1

