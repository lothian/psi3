
srcdir = @srcdir@
VPATH = @srcdir@

FC_SYMBOL = @FC_SYMBOL@

include ../MakeVars

CDEF += -DFC_SYMBOL=$(FC_SYMBOL)

TARGET = $(top_objdir)/lib/libPSI_qt.a

CSRC = \
blas_intfc.c       invert.c     ras_set.c       schmidt.c \
dirprd_block.c     mat_in.c     newmm_rking.c  reorder_qt.c    schmidt_add.c \
dot_block.c        mat_print.c  normalize.c    \
eri.c              pople.c        solve_pep.c \
fill_sym_matrix.c  probabil.c     rootfind.c      timer.c \
filter.c           lapack_intfc.c  sort.c      slaterdset.c   david.c \
get_frzpi.c

DEPENDINCLUDE = qt.h

LIBOBJ = $(CSRC:%.c=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
include $(LIBOBJ:%.o=%.d)
endif

install_inc:: qt.h
	$(MKDIRS) $(incdir)/libqt
	$(INSTALL_INCLUDE) $^ $(incdir)/libqt

install_man:: libqt.txt
	$(MKDIRS) $(txtdir)
	$(INSTALL_INCLUDE) $^ $(txtdir)
