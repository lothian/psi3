
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

CDEF += -DBUFF=0 -DSITEDIR=\"$(datadir)\"

TARGET = $(top_objdir)/lib/libqt.a

CSRC = \
blas_intfc.c       invert.c     ras_set.c       schmidt.c \
dirprd_block.c     mat_in.c     newmm_rking.c  reorder_qt.c    schmidt_add.c \
dot_block.c        mat_print.c  normalize.c    reorder_ras.c   sem.c \
eri.c              newmm.c      pople.c        reorder_ras2.c  solve_pep.c \
fill_sym_matrix.c  newmm2.c     probabil.c     rootfind.c      timer.c

FSRC = dgemmc.F

DEPENDINCLUDE = qt.h

LIBOBJ = $(CSRC:%.c=%.o) $(FSRC:%.F=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
include $(LIBOBJ:%.o=%.d)
endif

install_inc:: qt.h
	$(MKDIRS) $(objincdir)
	$(INSTALL_INCLUDE) $^ $(objincdir)

install_man:: libqt.txt
	$(MKDIRS) $(txtdir)
	$(INSTALL_INCLUDE) $^ $(txtdir)

