
SHELL = /bin/sh

#
# Initialize variables that have been figured out by configure.
#

target = @target@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
incdir = @includedir@
docdir = @docdir@
manext = 1
mandir = @mandir@
htmldir = @htmldir@
txtdir = @txtdir@
datarootdir = @datarootdir@
datadir = @datadir@
pkgdatadir=@pkgdatadir@
top_srcdir = @top_srcdir@
# this file is used by the subdirectories, so top_objdir is relative to those
top_objdir = ../../..
objincdir = @objincdir@

PWD = $(shell pwd)
RANLIB = @RANLIB@
YACC = @YACC@
CC = @CC@
CXX = @CXX@
LD = $(CXX)
MKDIRS = @MKDIRS@
INSTALL = @INSTALL@
INSTALL_INCLUDE = @INSTALL_INCLUDE@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_PROGRAM = @INSTALL_PROGRAM@

PSICPPFLAGS = @PSICPPFLAGS@
CPPFLAGS = @CPPFLAGS@

CDEF = @CDEF@
#CDEF += -DUSE_LIBCHKPT
COPT = @COPT@
COTH = @COTH@
CDBG = @CDBG@
CINC = @CINC@

CXXDEF = @CXXDEF@      
#CXXDEF += -DUSE_LIBCHKPT 
CXXOPT = @CXXOPT@
CXXOTH = @CXXOTH@
CXXDBG = @CXXDBG@
CXXINC = @CXXINC@

# The suffix generated by the -M compiler option
CCDEPENDSUF = @CCDEPENDSUF@
CXXDEPENDSUF = @CXXDEPENDSUF@
CCDEPENDFLAGS = @CCDEPENDFLAGS@
CXXDEPENDFLAGS = @CXXDEPENDFLAGS@
CCDEPEND = $(CC)
CXXDEPEND = $(CXX)

# Flags for Ruby location
RUBYLIB = @RUBYLIB@
RUBYINC = @RUBYINC@
HAVE_RUBY = @HAVE_RUBY@
RUBYLDFLAGS = @RUBYLDFLAGS@

# flags to be passed to compilers
CFLAGS = @CFLAGS@
CXXFLAGS = @CXXFLAGS@

# set up suffixes needed for various files
OBJSUF = @OBJSUF@
LIBSUF = @LIBSUF@

FLIBS = @FLIBS@
BLAS = @BLAS@ $(FLIBS)
LAPACK = @LAPACK@ $(BLAS)
LDFLAGS = @LDFLAGS@
LDLIBS = @LDLIBS@
vpath %.a $(top_objdir)/lib:$(libdir)

#
# have target default to the name of the directory we're in
#
PSITARGET = $(shell basename `pwd`)

# Force -lx to only look for libx.a.
.LIBPATTERNS = lib%.a

#
# Initialize other variables.
#

# Force -lx to only look for libx.a.
.LIBPATTERNS = lib%.a

