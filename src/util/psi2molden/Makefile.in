srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

LDLIBS += $(LAPACK) $(BLAS)

PSILIBS = -lPSI_qt -lPSI_chkpt -lPSI_psio -lPSI_ciomr -lPSI_ipv1

CSRC = \
psi2molden.c

BINOBJ = $(CSRC:%.c=%.o)
