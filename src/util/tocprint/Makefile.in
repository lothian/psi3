
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

CSRC = tocprint.c cc2unit.c
BINOBJ = $(CSRC:%.c=%.o)
PSILIBS = -lPSI_psio -lPSI_ciomr -lPSI_ipv1
