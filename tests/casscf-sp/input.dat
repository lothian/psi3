% 6-31G** H2O Test CASSCF Energy Point

psi: (
  jobtype = sp
  check = false
)

default: (
  label = "6-31G** CASSCF H2O"
  memory = (20.0 MB)

  symmetry = c2v
  wfn = detcas
  reference = rhf
  opentype = none 
  dertype = none
  docc = (3 0 1 1)
  frozen_docc = (1 0 0 0)
  frozen_uocc = (8 2 3 5)

  files: (
    default: ( name = "h2o" )
  )
)

input: (
  basis = 631Gss
  units = angstrom
  zmat = (
    (o)
    (h 1 1.00)
    (h 1 1.00 2 103.1)
  )
  origin = (0.0 0.0 0.0)
)

basis: (
  oxygen:631Gss = (
    (get "631GST")
)
  hydrogen:631Gss = (
    (get "631GST")
    (get "STAR1")
  )
)

scf: (
  maxiter = 100
  convergence = 12
)

detci: (
  fci = true
  guess_vector = dfile
  ex_lvl = 8
)

detcas: (
  convergence = 5
)


