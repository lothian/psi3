
%BH calculation
%

default: (
  label = "BH cc-pVQZ CCSD(T)"
  memory = (100.0 MB)
  charge = 0
  multp = 1
  reference = rhf
  symmetry = c2v
  wfn = ccsd_t
  dertype = none
  docc = (3 0 0 0)
  frozen_docc = (1 0 0 0)
  puream = true

  % using the ~/.psirc file to get default files info
  files: (
    default: ( name = "bh" )
  )
)

input: (
  basis = ccpvqz
  units = angstroms
  geometry = (
    ( b      0.0000        0.0000        0.0000)
    ( h      0.0000        0.0000        1.0000)
      )
  origin = (0.0 0.0 0.0)
)


scf: (
  maxiter = 100
  convergence = 11
)


