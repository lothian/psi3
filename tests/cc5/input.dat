#%! RHF CCSD(T) Frozen-core energy of C4NH4

refnuc = 135.0921284884196
refscf = -208.2159793502
refccsd = -0.884280512675734
ref_t = -0.048772959402886
reftotal = -209.149032822278599

molecule c4nh4 {
    -1 1
    C         0.00000000     0.00000000     2.13868804
    N         0.00000000     0.00000000     4.42197911
    C         0.00000000     0.00000000    -0.46134192
    C        -1.47758582     0.00000000    -2.82593059
    C         1.47758582     0.00000000    -2.82593059
    H        -2.41269553    -1.74021190    -3.52915989
    H        -2.41269553     1.74021190    -3.52915989
    H         2.41269553     1.74021190    -3.52915989
    H         2.41269553    -1.74021190    -3.52915989
    bohr
}  

globals {
  wfn ccsd_t
  reference rhf
  docc [10, 1, 4, 3]
  basis aug-cc-pvtz

  convergence 10
  e_converge 10
  d_converge 10
}

activate(c4nh4)
mints()
energy('ccsd(t)')

compare_values(refnuc,   CN.nuclear_repulsion_energy(),  9, "Nuclear repulsion energy")
compare_values(refscf,   get_variable("SCF energy"),     9, "SCF energy")
compare_values(refccsd,  get_variable("CCSD energy"),    9, "CCSD contribution")
compare_values(ref_t,    get_variable("(T) energy"),     9, "(T) contribution")
compare_values(reftotal, get_variable("Current energy"), 9, "Total energy")

