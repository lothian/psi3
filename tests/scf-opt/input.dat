% 6-31G** H2O Test SCF optimization

psi: (
  check = false
)

default: (
  label = "6-31G** SCF H2O"
  memory = (20.0 MB)

  symmetry = c2v
  wfn = scf
  reference = rhf
  multp = 1
  dertype = first
  opt = true
  nopt = 20 
  docc = (3 0 1 1)

files: (
    default: ( name = "h2o" nvolume = 1
      volume1 = "/scratch/sherrill/"
      )
    file30: (nvolume = 1 volume1 = "./" )
    )
)

input: (
  basis = 631Gss
  units = angstrom
  zmat = (
    (o)
    (h 1 1.00)
    (h 1 1.00 2 103.1)
  )
  origin = (0.0 0.0 0.0)
)

basis: (
  oxygen:631Gss = (
    (get "631GST")
)
  hydrogen:631Gss = (
    (get "631GST")
    (get "STAR1")
  )
)

scf: (
  maxiter = 100
  convergence = 12
)

