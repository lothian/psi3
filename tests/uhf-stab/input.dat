% cc-pVDZ UHF CN stability analysis

psi: (
  check = false
  jobtype = sp
  exec = ($input $ints $scf $transqt $ccsort $stable $done)
)

default: (
  label = "cc-pVDZ UHF CN"

  wfn = ccsd
  reference = uhf
  multi = 2
  dertype = none

  docc = (4 0 1 1)
  socc = (1 0 0 0)
)

input: (
  basis = "cc-pVDZ"
  puream = true
  zmat = (
   (C)
   (N 1 R)
  )
  zvars = (
   (R 1.175)
  )
)
